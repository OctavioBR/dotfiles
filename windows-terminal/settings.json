{
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "copyFormatting": false,
  "copyOnSelect": false,
  "language": "en-US",
  "tabWidthMode": "equal",
  "windowingBehavior": "useExisting",
  "actions": [
    {
      "command": { "action": "copy", "singleLine": true },
      "keys": "ctrl+c"
    },{
      "command": "paste",
      "keys": "ctrl+v"
    },{
      "command": "scrollUpPage",
      "keys": "shift+pageup"
    },{
      "command": "scrollDownPage",
      "keys": "shift+pagedown"
    },{
      "command": "newTab",
      "keys": "ctrl+t"
    },{
      "command": "find",
      "keys": "ctrl+shift+f"
    },{
      "command": "toggleFocusMode",
      "keys": "alt+f"
    },{
      "command": "closePane",
      "keys": "ctrl+w"
    },{
      "command": { "action": "splitPane", "split": "horizontal" },
      "keys": "ctrl+shift+-"
    },{
      "command": { "action": "splitPane", "split": "vertical" },
      "keys": "ctrl+shift+plus"
    },{
      "command": { "action": "splitPane", "split": "auto", "splitMode": "duplicate" },
      "keys": "ctrl+shift+o"
    },{
      "command": { "action": "quakeMode" },
      "keys": "win+'"
    }
  ],
